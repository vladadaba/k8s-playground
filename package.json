{
  "name": "turborepo-microservices",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start:dev": "turbo run start:dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "turbo": "2.2.3"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
